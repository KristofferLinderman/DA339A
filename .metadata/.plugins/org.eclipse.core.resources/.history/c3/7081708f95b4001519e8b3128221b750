package yatzzy;

public class DnaStrand {

	private char current;

	public static String makeComplement(String dna) {
		String output = "";

		for (int i = 0; i < dna.length(); i++) {
			current = dna.charAt(i);

			if (current == 'A') {
				output += "T";
			} else if (current == 'C') {
				output += "G";
			} else if (current == 'T') {
				output += "A";
			} else if (current == 'G') {
				output += "C";
			}
		}

		return output;
	}
}